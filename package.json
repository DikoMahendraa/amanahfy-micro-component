    "formik": "^2.4.6",
    "@headlessui/react": "^2.0.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dropzone": "^14.2.9",
    "react-icons": "^5.3.0",
    "react-image-crop": "^11.0.7",
    "react-quill": "^2.0.0",
    "sanitize-html": "^2.13.1",
    "uuid": "^10.0.0",
    "yup": "^1.4.0"
  },
  "keywords": [
    "react",
    "component",
    "ui",
    "amanahfy-ui-components"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "description": "A reusable React component library for Amanahfy UK",
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --external react --external react-dom --external @headlessui/react",
    "start": "react-scripts start",
    "bundle": "tsup src/index.ts",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format:check": "prettier --check ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/preset-create-react-app": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@swc/core": "^1.10.7",
    "@types/date-fns": "^2.6.3",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-image-crop": "^8.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/sanitize-html": "^2.13.0",
    "@types/uuid": "^10.0.0",
    "@types/yup": "^0.32.0",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-storybook": "^0.12.0",
    "postcss": "^8.4.47",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "storybook": "^8.6.12",
    "tailwindcss": "^3.4.13",
