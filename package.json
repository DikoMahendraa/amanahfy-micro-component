  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.0.0",
    "@headlessui/react": "^2.0.0"
  },
  "dependencies": {
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "clsx": "^2.1.1",
